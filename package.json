{
  "name": "midnight-themepack",
  "displayName": "Midnight Moon Theme Collection",
  "description": "A collection of visually appealing, eye-friendly themes with cool blue-ish colors for Visual Studio Code.",
  "version": "1.0.1",
  "engines": {
    "vscode": "^1.91.0"
  },
  "publisher": "noxire",
  "repository": {
    "type": "git",
    "url": "https://github.com/Noxire-Hash/midnight-theme"
  },
  "license": "MIT",
  "icon": "./img/midnightLogo.png",
  "categories": [
    "Themes"
  ],
  "contributes": {
    "debuggers": [
      {
        "type": "node-debug",
        "runtime": "node",
        "runtimeArgs": [
          "--inspect"
        ],
        "args": [
          "--inspect"
        ]
      }
    ],
    "themes": [
      {
        "label": "Midnight Moon Classic",
        "uiTheme": "vs-dark",
        "path": "./themes/midnight-moon.json"
      },
      {
        "label": "Midnight Moon Pro",
        "uiTheme": "vs-dark",
        "path": "./themes/midnight-moon-pro.json"
      },
      {
        "label": "Midnight Moon Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/midnight-dark-moon.json"
      },
      {
        "label": "Midnight Moon Ukiyo",
        "uiTheme": "vs-dark",
        "path": "./themes/midnight-moon-ukiyo.json"
      }
    ]
  }
}
