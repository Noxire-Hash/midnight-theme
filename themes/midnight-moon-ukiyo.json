  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#727169",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#98BB6C"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language", "constant.character"],
      "settings": {
        "foreground": "#FF9E3B"
      }
    },
    {
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#DCD7BA"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#957FB8"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#7FB4CA"
      }
    },
    {
      "scope": ["entity.name.type", "support.type"],
      "settings": {
        "foreground": "#7AA89F"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#E6C384"
      }
    },
    {
      "scope": ["punctuation", "meta.brace"],
      "settings": {
        "foreground": "#C8C093"
      }
    },
    {
      "scope": ["entity.name.tag", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#957FB8"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#7FB4CA"
      }
    },
    {
      "scope": [
        "support.type.primitive.ts",
        "support.type.builtin.ts",
        "support.type.primitive.tsx",
        "support.type.builtin.tsx"
      ],
      "settings": {
        "foreground": "#7AA89F",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["meta.decorator", "punctuation.decorator"],
      "settings": {
        "foreground": "#957FB8",
        "fontStyle": "italic"
      }
    },

    {
      "scope": ["support.class.component"],
      "settings": {
        "foreground": "#7AA89F",
        "fontStyle": "bold"
      }
    },

    {
      "scope": [
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.class.scss"
      ],
      "settings": {
        "foreground": "#98BB6C",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "entity.other.attribute-name.id",
      "settings": {
        "foreground": "#7FB4CA",
        "fontStyle": "bold"
      }
    }
  ],

  "semanticHighlighting": true,
  "semanticTokenColors": {
    "property": "#DCD7BA",
    "method": "#7FB4CA",
    "function": "#7FB4CA",
    "class": "#7AA89F",
    "interface": "#7AA89F",
    "enum": "#7AA89F",
    "type": "#7AA89F",
    "variable": "#DCD7BA",
    "parameter": "#E6C384",
    "string": "#98BB6C",
    "keyword": "#957FB8",
    "number": "#FF9E3B",
    "regexp": "#957FB8",
    "operator": "#957FB8",
    "decorator": "#957FB8",
    "namespace": "#7AA89F",

    "typeParameter": "#7AA89F",
    "class.declaration": {
      "foreground": "#7AA89F",
      "bold": true
    },
    "interface.declaration": {
      "foreground": "#7AA89F",
      "bold": true
    },
    "function.declaration": {
      "foreground": "#7FB4CA",
      "bold": true
    },
    "method.declaration": {
      "foreground": "#7FB4CA",
      "bold": true
    },
    "variable.defaultLibrary": {
      "foreground": "#957FB8",
      "italic": true
    },
    "property.readonly": {
      "foreground": "#E6C384",
      "bold": true
    },

    "component": {
      "foreground": "#7AA89F",
      "bold": true
    },
    "jsx.tag": "#957FB8",
    "jsx.component": "#7AA89F",
    "jsx.attributeName": "#7FB4CA"
  }
}
