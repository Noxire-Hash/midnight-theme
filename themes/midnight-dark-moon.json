  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#7A8A99",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "constant",
        "entity.name.constant",
        "variable.other.constant",
        "variable.language"
      ],
      "settings": {
        "foreground": "#FFD580"
      }
    },
    {
      "scope": ["entity", "entity.name"],
      "settings": {
        "foreground": "#E6B673"
      }
    },
    {
      "scope": "variable.parameter.function",
      "settings": {
        "foreground": "#F0A8B0"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#39BAE6"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#FF8F40",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["storage", "storage.type"],
      "settings": {
        "foreground": "#FF8F40"
      }
    },
    {
      "scope": [
        "storage.modifier.package",
        "storage.modifier.import",
        "storage.type.java"
      ],
      "settings": {
        "foreground": "#C0BEB9"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#C2E98F"
      }
    },
    {
      "scope": "support",
      "settings": {
        "foreground": "#FFD580"
      }
    },
    {
      "scope": "meta.property-name",
      "settings": {
        "foreground": "#FFD580"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#F07178"
      }
    },
    {
      "scope": "variable.other",
      "settings": {
        "foreground": "#C0BEB9"
      }
    },
    {
      "scope": [
        "invalid.broken",
        "invalid.deprecated",
        "invalid.illegal",
        "invalid.unimplemented"
      ],
      "settings": {
        "fontStyle": "italic underline",
        "foreground": "#FF3333"
      }
    },
    {
      "scope": "string variable",
      "settings": {
        "foreground": "#C2E98F"
      }
    },
    {
      "scope": ["source.regexp", "string.regexp"],
      "settings": {
        "foreground": "#95E6CB"
      }
    },
    {
      "scope": "support.constant",
      "settings": {
        "foreground": "#FFD580"
      }
    },
    {
      "scope": "meta.module-reference",
      "settings": {
        "foreground": "#FFD580"
      }
    },
    {
      "scope": "punctuation.definition.list.begin.markdown",
      "settings": {
        "foreground": "#F07178"
      }
    },
    {
      "scope": "markup.inline.raw.string.markdown",
      "settings": {
        "foreground": "#C2E98F"
      }
    },
    {
      "scope": "punctuation.definition.raw.markdown",
      "settings": {
        "foreground": "#FFD580"
      }
    },
    {
      "scope": "variable.other.property",
      "settings": {
        "foreground": "#4DC2FF"
      }
    },
    {
      "scope": "variable.other.object",
      "settings": {
        "foreground": "#C0BEB9"
      }
    },
    {
      "scope": "entity.name.function.method",
      "settings": {
        "foreground": "#4DC2FF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "entity.name.type.class",
      "settings": {
        "foreground": "#FFB454",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "entity.name.type.interface",
      "settings": {
        "foreground": "#5CCFE6",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "entity.name.type.enum",
      "settings": {
        "foreground": "#FFB454",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#4DC2FF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#FFB454"
      }
    },
    {
      "scope": "support.function",
      "settings": {
        "foreground": "#39BAE6"
      }
    },
    {
      "scope": "support.type",
      "settings": {
        "foreground": "#5CCFE6"
      }
    },
    {
      "scope": "support.class",
      "settings": {
        "foreground": "#FFB454"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#FF9580"
      }
    },
    {
      "scope": "meta.brace",
      "settings": {
        "foreground": "#C0BEB9"
      }
    },
    {
      "scope": "punctuation.definition.parameters",
      "settings": {
        "foreground": "#C0BEB9"
      }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "property": "#4DC2FF",
    "method": {
      "foreground": "#4DC2FF",
      "bold": true
    },
    "class": {
      "foreground": "#FFB454",
      "bold": true
    },
    "interface": {
      "foreground": "#5CCFE6",
      "bold": true
    },
    "enum": {
      "foreground": "#FFB454",
      "bold": true
    },
    "type": "#FFB454",
    "function": {
      "foreground": "#4DC2FF",
      "bold": true
    },
    "variable": "#F07178",
    "parameter": "#F0A8B0",
    "string": "#C2E98F",
    "keyword": {
      "foreground": "#FF8F40",
      "bold": true
    },
    "number": "#FF9580",
    "regexp": "#95E6CB",
    "operator": "#C0BEB9"
  }
}
