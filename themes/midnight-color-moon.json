    "sideBar.border": "#1A1D2E",
    "sideBarSectionHeader.background": "#151724",
    "sideBarSectionHeader.foreground": "#D8E1FF",

    "list.activeSelectionBackground": "#3A3F5C",
    "list.activeSelectionForeground": "#D8E1FF",
