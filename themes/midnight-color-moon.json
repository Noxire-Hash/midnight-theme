    "inputValidation.infoBorder": "#5A84F8",
    "inputValidation.warningBackground": "#5E4C24",
    "inputValidation.warningBorder": "#FFDC7B",
    "inputValidation.errorBackground": "#5E2D3B",
    "inputValidation.errorBorder": "#FF6380",

    "editorHoverWidget.background": "#242A45",
    "editorHoverWidget.border": "#3A3F5C",
    "editorHoverWidget.foreground": "#D8E1FF",
