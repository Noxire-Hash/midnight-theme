    "list.hoverBackground": "#3A3F5C",
    "list.highlightForeground": "#92BAFF",

    "dropdown.background": "#151724",
    "dropdown.foreground": "#D8E1FF",
    "dropdown.border": "#3A3F5C",
    "menu.background": "#151724",
    "menu.foreground": "#D8E1FF",
    "menu.selectionBackground": "#3A3F5C",
    "menu.selectionForeground": "#D8E1FF",
    "menubar.selectionBackground": "#3A3F5C",
    "menubar.selectionForeground": "#D8E1FF",

    "editorGroupHeader.tabsBackground": "#151724",
    "tab.inactiveBackground": "#151724",
