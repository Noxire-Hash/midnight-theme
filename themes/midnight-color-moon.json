    "minimapSlider.hoverBackground": "#44475ADD",
    "minimapSlider.activeBackground": "#44475AFF",

    "editor.findMatchBackground": "#5E6D9C",
    "editor.findMatchHighlightBackground": "#5E6D9C90",
    "editor.findRangeHighlightBackground": "#415174A0",

    "editorBracketMatch.background": "#41517460",
    "editorBracketMatch.border": "#92BAFF60",
