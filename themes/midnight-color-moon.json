    "titleBar.activeForeground": "#D8E1FF",

    "editor.lineHighlightBackground": "#1E223A",
    "editor.lineHighlightBorder": "#3D3D5D",
    "editor.selectionBackground": "#3A3F5C",
    "editor.selectionHighlightBackground": "#415174A0",
    "editor.hoverHighlightBackground": "#415174A0",

    "editorIndentGuide.background": "#4B5573",
    "editorIndentGuide.activeBackground": "#6B7290",
    "editorGutter.background": "#1A1D2E",
    "editorLineNumber.foreground": "#6C7A9A",
    "editorLineNumber.activeForeground": "#92BAFF",

    "gitDecoration.modifiedResourceForeground": "#FFDC7B",
    "gitDecoration.deletedResourceForeground": "#FF6380",
