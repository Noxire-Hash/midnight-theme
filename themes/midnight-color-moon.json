    "editorLink.activeForeground": "#92BAFF",
    "editorLightBulb.foreground": "#FFDC7B",
    "editorLightBulbAutoFix.foreground": "#5CD2B9"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#7F8BB3",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string", "string.quoted", "string.template"],
      "settings": {
        "foreground": "#6AFF9F"
      }
    },
    {
      "scope": ["constant", "variable.other.constant", "variable.language"],
      "settings": {
        "foreground": "#FFB07B"
      }
    },
    {
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#92BAFF"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#FF7AC6",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#82AAFF",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#FFD700",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#FF7AC6",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#C9A3FF"
      }
    },
    {
      "scope": "support.type.property-name",
      "settings": {
        "foreground": "#7DCFFF"
      }
    },
    {
      "scope": ["markup.heading", "markup.bold"],
      "settings": {
        "foreground": "#FFD700",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "foreground": "#82AAFF",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#6AFF9F"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#FF7AC6"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#FFB07B"
      }
    },
    {
      "scope": [
        "punctuation.definition.string",
        "punctuation.support.type.property-name"
      ],
      "settings": {
        "foreground": "#8BE9FD"
      }
    },
    {
      "scope": "meta.object-literal.key",
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "scope": "support.constant",
      "settings": {
        "foreground": "#FFB07B"
      }
    },
    {
      "scope": "support.type.primitive",
      "settings": {
        "foreground": "#FFD700"
      }
    },
    {
      "scope": "meta.decorator",
      "settings": {
        "foreground": "#BD93F9"
      }
    },
    {
      "scope": "constant.language",
      "settings": {
        "foreground": "#FF9580",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "entity.other.inherited-class",
      "settings": {
        "foreground": "#FFD700",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#8BE9FD",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "entity.name.section",
      "settings": {
        "foreground": "#82AAFF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "punctuation.definition",
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#D0D9F5"
      }
    },
    {
      "scope": ["punctuation.accessor", "keyword.operator.accessor"],
      "settings": {
        "foreground": "#FF9580"
      }
    },
    {
      "scope": "string.regexp",
      "settings": {
        "foreground": "#FF7AC6"
      }
    },
    {
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#FFB07B"
      }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "property": "#FFA59E",
    "method": "#82AAFF",
    "function": "#82AAFF",
    "class": "#FFD700",
    "interface": "#FFB88C",
    "enum": "#FFB88C",
    "type": "#FFE08C",
    "variable": "#8BE9FD",
    "parameter": "#8CDFFF",
    "string": "#6AFF9F",
    "keyword": "#FF8AD8",
    "number": "#C9A3FF",
    "regexp": "#FF8AD8",
    "operator": "#FF7AC6",
    "decorator": "#C9A3FF",
    "namespace": "#8CDFFF",
    "typeParameter": "#FFE08C",
    "member": "#FFA59E",
    "macro": "#FF8AD8"
  }
}
